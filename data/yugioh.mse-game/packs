
############################################################## Card pack items
pack type:
	name: rare
	filter: card.rarity != "common"
pack type:
	name: common
	filter: card.rarity == "common"
pack type:
	name: gold
	filter: card.rarity == "gold tech" or card.rarity == "gold rare"
pack type:
	name: card
	filter: card.number != ""
############################################################## Card packs
pack type:
	name: booster pack
	item:
		name: rare
		amount: 1
	item:
		name: common
		amount: 8
